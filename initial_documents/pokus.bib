@article{isi:000285252600040,
    author = "Bednarcik, J. and Curfs, C. and Sikorski, M. and Franz, H. and Jiang, J. Z.",
    title = "Thermal expansion of La-based BMG studied by in situ high-energy X-ray diffraction",

@article{isi:000403968300018,
    author = "Hansen, Bjarne R. S. and Tumanov, Nikolay and Santoru, Antonio and Pistidda, Claudio and Bednarcik, Jozef and Klassen, Thomas and Dornheim, Martin and Filinchuk, Yaroslav and Jensen, Torben R.",
    title = "Synthesis, structures and thermal decomposition of ammine MxB12H12 complexes (M = Li, Na, Ca)",
    journal = "DALTON TRANSACTIONS",
